{
  "name": "advent-of-code-2020",
  "version": "1.0.0",
  "private": true,
  "author": "Mackie Underdown",
  "license": "MIT",
  "devDependencies": {
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged --concurrent false"
    }
  },
  "lint-staged": {
    "*": "bin/lint",
    "*.{json,md}": "prettier --write"
  },
  "prettier": {
    "jsxSingleQuote": true,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
